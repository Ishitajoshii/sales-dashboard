{
  "name": "sales-dashboard",
  "version": "1.0.0",
  "description": "A sales dashboard built with React, Node.js, Express, and Knex.js.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon src/server/index.js",
    "client": "npm start --prefix src/client",
    "build": "npm install --prefix src/client && npm run build --prefix src/client",
    "test": "jest --watchAll --verbose --coverage",
    "test:client": "jest --watchAll --verbose --coverage --config ./jest.config.client.js",
    "test:server": "jest --watchAll --verbose --coverage --config ./jest.config.server.js",
    "knex": "knex",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run"
  },
  "keywords": [
    "react",
    "node",
    "express",
    "knex",
    "sales",
    "dashboard"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}